generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}



model Todo {
  id           Int        @id @default(autoincrement())
  title        String
  description  String?    @db.VarChar(256)

  createdAt   DateTime    @default(now()) @map("created_at")

  @@map("todo")
}
